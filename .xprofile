# Xsettings
xsettingsd &

# Disable beep
xset -b

# Set keyboad layout
setxkbmap -layout de

# Change caps to escape
setxkbmap -option caps:escape &

# Compositor
picom &

# Authorization Manager
/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &

#Network Manager
nm-applet &

#Bluetooth Manager
blueman-applet &

# Notification Manager
dunst &
